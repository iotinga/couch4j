<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info" name="JAKARTATEMPLATE">
  <Appenders>
    <Console name="STDERR" target="SYSTEM_OUT">
      <PatternLayout pattern="%highlight{%d [%t] %-5level: %msg%n%throwable}" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="STDERR" />
    </Root>
  </Loggers>
</Configuration>